{# Enlazar este fichero con la plantilla correspondiente #}
{% extends "layouts/public.twig" %}

{% block title %}Index
{% endblock %}



{% block content %}


	{# carousel #}

	<div id="carou" class="carousel slide bg-dark" data-ride="carousel">
		<ol class="carousel-indicators">
			<li data-target="#carou" data-slide-to="0" class="active"></li>
			<li data-target="#carou" data-slide-to="1"></li>
			<li data-target="#carou" data-slide-to="2"></li>
		</ol>
		<div class="carousel-inner">
			<div class="carousel-item active">
				<img class="img-fluid" src="./assets/img/carrou/3.jpg" alt="First slide">
			</div>
			<div class="carousel-item">
				<img class="img-fluid" src="./assets/img/carrou/2.jpg" alt="Second slide">
			</div>
			<div class="carousel-item">
				<img class="img-fluid" src="./assets/img/carrou/3.jpg" alt="Third slide">
			</div>
		</div>
		<a class="carousel-control-prev" href="#carou" role="button" data-slide="prev">
			<span class="carousel-control-prev-icon" aria-hidden="true"></span>
			<span class="sr-only">Previous</span>
		</a>
		<a class="carousel-control-next" href="#carou" role="button" data-slide="next">
			<span class="carousel-control-next-icon" aria-hidden="true"></span>
			<span class="sr-only">Next</span>
		</a>
	</div>




{% endblock %}
